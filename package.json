{
  "name": "guide-intelligence-dashboard",
  "version": "1.0.0",
  "description": "React dashboard for Guide Intelligence with agent performance monitoring",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently \"npm run start\" \"npm run backend\"",
    "backend": "cd backend && python app.py",
    "mcp": "cd MCP-SERVER && python main.py --stdio",
    "install-all": "npm install && cd backend && pip install -r requirements.txt && cd ../MCP-SERVER && pip install -r requirements.txt"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "axios": "^1.6.0",
    "recharts": "^2.8.0",
    "lucide-react": "^0.294.0",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
